{
  "name": "yurigobatto/doctrine-mongo-odm",
  "description": "DoctrineMongoODM component for (Micro-)Framework",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "expressive",
    "http",
    "middleware",
    "psr",
    "psr-7",
    "doctrine",
    "mongodb",
    "odm"
  ],
  "authors": [
    {
      "name": "Yuri Gobatto",
      "email": "yurigobatto@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "psr/container": "^2.0.1",
    "doctrine/mongodb-odm": "^2.2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "squizlabs/php_codesniffer": "^3.6.0",
    "phpspec/prophecy": "^1.10.3",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "YuriGobatto\\Component\\DoctrineMongoODM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YuriGobatto\\Test\\Component\\DoctrineMongoODM\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "scripts": {
    "check": [
      "@cs",
      "@test"
    ],
    "cs": "phpcs",
    "test": "phpunit"
  }
}
